<div class="jumbotron text-center">
    <h1>All Assets</h1>
</div>


<table class="table table-bordered table-hover table-striped">
    <thead>
        <tr>
            <th>Asset Name</th>
            <th>Description</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        
        <% for (var event of events) { %>
            <tr>
                <td><%= event.name %></td>
                <td><%= event.description %></td>
                <td>
                    <a href="/<%= event.slug %>" class="btn btn-sm btn-primary">View</a>
                    <a href="/<%= event.slug %>/edit" class="btn btn-sm btn-primary">Edit</a>
                    <a href="/<%= event.slug %>/delete" class="btn btn-sm btn-danger">Delete</a>
                </td>
            </tr>
        <% } %>

    </tbody>
</table>